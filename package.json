{
  "name": "docusaurus-2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "reference-docs": "cd content-repo && ./create-docs.sh",
    "test": "cd content-repo && pipenv install --dev && pipenv run flake8 *.py && pipenv run mypy gendocs.py && pipenv run pytest -v",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "heapstats": "node -e 'console.log(\"Node heap stats: \", v8.getHeapStatistics())'",
    "build-docusaurus": "cp -RPv docs/doc_imgs/* static/doc_imgs/ && replace '(\\.\\.)?(/\\.\\.){0,2}(/?\\.\\./docs)?/doc_imgs' '/doc_imgs' docs/* --include='*.md*' --recursive && docusaurus build",
    "build": "npm run reference-docs && npm run heapstats && npm run build-docusaurus",
    "findorphans": "for i in `cd docs && find . -name '*.md' | cut -d '.' -f2 | cut -d'/' -f2-`; do grep $i sidebars.js 1>/dev/null || echo \"Document not in sidebar: $i\"; done",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "node": "node"
  },
  "dependencies": {
    "@docusaurus/core": "3.0.1",
    "@docusaurus/mdx-loader": "3.0.1",
    "@docusaurus/plugin-content-blog": "3.0.1",
    "@docusaurus/plugin-content-docs": "3.0.1",
    "@docusaurus/plugin-content-pages": "3.0.1",
    "@docusaurus/plugin-google-analytics": "3.0.1",
    "@docusaurus/plugin-google-gtag": "3.0.1",
    "@docusaurus/plugin-sitemap": "3.0.1",
    "@docusaurus/preset-classic": "3.0.1",
    "@docusaurus/theme-classic": "3.0.1",
    "@docusaurus/theme-live-codeblock": "3.0.1",
    "@docusaurus/theme-search-algolia": "3.0.1",
    "@docusaurus/types": "3.0.1",
    "@docusaurus/utils": "3.0.1",
    "@mdx-js/mdx": "3.0.0",
    "@mdx-js/react": "*",
    "algoliasearch": "*",
    "classnames": "*",
    "copyfiles": "*",
    "mini-css-extract-plugin": "*",
    "query-string": "*",
    "react": "18.1.0",
    "react-collapsible": "2.10.0",
    "react-dom": "18.1.0",
    "react-particles": "*",
    "react-player": "*",
    "react-responsive": "*",
    "react-scroll-to-top": "3.0.0",
    "react-select": "5.8.0",
    "react-slideshow-image": "4.3.0",
    "replace": "*",
    "showdown": "*"
  },
  "devDependencies": {
    "js-string-escape": "*",
    "node-fetch": "*",
    "node-plop": "*",
    "plop": "*",
    "xmlhttprequest": "*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
