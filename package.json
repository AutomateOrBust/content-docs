{
  "name": "docusaurus-2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "reference-docs": "cd content-repo && ./create-docs.sh",
    "test": "cd content-repo && pipenv install --dev && pipenv run flake8 *.py && pipenv run mypy gendocs.py && pipenv run pytest -v",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "heapstats": "node -e 'console.log(\"Node heap stats: \", v8.getHeapStatistics())'",
    "build-docusaurus": "cp -RPv docs/doc_imgs/* static/doc_imgs/ && replace '(\\.\\.)?(/\\.\\.){0,2}(/?\\.\\./docs)?/doc_imgs' '/doc_imgs' docs/* --include='*.md*' --recursive && docusaurus build",
    "build": "docusaurus build --dev",
    "findorphans": "for i in `cd docs && find . -name '*.md' | cut -d '.' -f2 | cut -d'/' -f2-`; do grep $i sidebars.js 1>/dev/null || echo \"Document not in sidebar: $i\"; done",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "node": "node"
  },
  "dependencies": {
    "@docusaurus/core": "^3.2.0",
    "@docusaurus/mdx-loader": "^3.2.0",
    "@docusaurus/plugin-content-blog": "^3.2.0",
    "@docusaurus/plugin-content-docs": "^3.2.0",
    "@docusaurus/plugin-content-pages": "^3.2.0",
    "@docusaurus/plugin-google-analytics": "^3.2.0",
    "@docusaurus/plugin-google-gtag": "^3.2.0",
    "@docusaurus/plugin-sitemap": "^3.2.0",
    "@docusaurus/preset-classic": "^3.2.0",
    "@docusaurus/theme-classic": "^3.2.0",
    "@docusaurus/theme-live-codeblock": "^3.2.0",
    "@docusaurus/theme-search-algolia": "^3.2.0",
    "@docusaurus/types": "^3.2.0",
    "@docusaurus/utils": "^3.2.0",
        "@mdx-js/mdx": "3.0.1",
        "@mdx-js/react": "3.0.1",
        "algoliasearch": "4.23.2",
        "classnames": "2.5.1",
        "copyfiles": "2.4.1",
        "mini-css-extract-plugin": "2.8.1",
        "query-string": "9.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-collapsible": "2.10.0",
        "react-particles": "2.12.2",
        "react-player": "2.15.1",
        "react-responsive": "10.0.0",
        "react-scroll-to-top": "3.0.0",
        "react-select": "5.8.0",
        "react-slideshow-image": "4.3.1",
        "replace": "1.2.2",
        "showdown": "2.1.0",
        "unist-util-visit": "5.0.0"
  },
  "devDependencies": {
    "js-string-escape": "1.0.1",
    "node-fetch": "3.3.2",
    "node-plop": "0.32.0",
    "plop": "4.0.1",
    "xmlhttprequest": "1.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module"
}
